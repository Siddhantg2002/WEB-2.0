{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@Components/*": [
                "src/Components/*"
            ],
            "@Layouts/*": [
                "src/Layouts/*"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@Features/*": [
                "src/Components/Features/*"
            ],
            "@Logos/*": [
                "src/Components/Logos/*"
            ]
        }
    }
}