{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@Components/*": [
                "src/Components/*"
            ],
            "@Layouts/*": [
                "src/Layouts/*"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@YourBlogsContent/*": [
                "src/Components/YourBlogsContent/*"
            ]
        }
    }
}