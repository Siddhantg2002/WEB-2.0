{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@Components/*": [
                "src/Components/*"
            ],
            "@Layouts/*": [
                "src/Layouts/*"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@Global/*": [
                "src/Global/*"
            ],
            "@ContactUs/*": [
                "src/Components/ContactUs/*"
            ],
            "@Pricing/*": [
                "src/Components/Pricing/*"
            ],
            "@T&C/*": [
                "src/Components/T&C/*"
            ],
            "@ForgetPassword/*": [
                "src/Components/ForgetPassword/*"
            ],
            "@VerifyOTP/*": [
                "src/Components/VerifyOTP/*"
            ],
            "@ResetPassword/*": [
                "src/Components/ResetPassword/*"
            ]
        }
    }
}